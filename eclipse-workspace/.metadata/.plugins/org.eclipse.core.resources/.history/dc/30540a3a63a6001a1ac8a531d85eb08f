#ifndef NODE_H_
#define NODE_H_

#include <iostream>

using namespace std;

class Node {
public:
	Node();
	virtual ~Node();

	int data;
	Node *next;

	//Methods
	void print_linkedlist(Node* n) const;
	int list_search(const Node *n, int item);

};

#endif /* NODE_H_ */
