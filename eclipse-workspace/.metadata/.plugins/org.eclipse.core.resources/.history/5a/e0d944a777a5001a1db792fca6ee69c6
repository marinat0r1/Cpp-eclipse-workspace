#ifndef ZOOTIER_H_
#define ZOOTIER_H_

class Zootier {
public:

	enum gefaehrlichkeit{SEHR_GEFAEHRLICH, GEFAEHRLICH, UNGEFAEHRLICH};

	Zootier();
	virtual ~Zootier();

	//Getter
	double getGewicht();

	//Setter
	void setGewicht(double _gewicht);

private:
	double gewicht;
};


#endif /* ZOOTIER_H_ */
